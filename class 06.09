#include <iostream>
using namespace std;

template <class T>
class Cola {
    T A[10];
    int* head;
    int* tail;

    bool full() const;
    bool empty() const;

public:
    Cola();
    bool push(const T& val);
    bool pop(T& val);
};

template <class T>
Cola<T>::Cola() : head(nullptr), tail(nullptr) {}

template <class T>
bool Cola<T>::full() const {
    return (head == A && tail == A + 9) || (head && tail && head == tail + 1);
}

template <class T>
bool Cola<T>::empty() const {
    return head == nullptr && tail == nullptr;
}

template <class T>
bool Cola<T>::push(const T& val) {
    if (full()) return false;
    if (empty()) {
        head = tail = A;
        *tail = val;
    } else {
        if (tail == A + 9) {
            if (head != A) {
                tail = A;
            } else {
                return false;
            }
        } else {
            tail++;
        }
        *tail = val;
    }
    return true;
}

template <class T>
bool Cola<T>::pop(T& val) {
    if (empty()) return false;
    val = *head;
    if (head == tail) { // La cola se vac√≠a
        head = tail = nullptr;
    } else if (head == A + 9) {
        head = A;
    } else {
        head++;
    }
    return true;
}

int main() {
    Cola<int> q;
    for(int i = 0; i < 10; i++) q.push(i);
    int val;
    while(q.pop(val)) cout << val << ' ';
    cout << endl;
    return 0;
}
