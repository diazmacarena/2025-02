#include <iostream>
using namespace std;

void multiplicarMatrices(int* A, int* B, int* C, int r1, int c1, int c2) {
    for (int i = 0; i < r1; ++i)
        for (int j = 0; j < c2; ++j) {
            *(C + i * c2 + j) = 0;
            for (int k = 0; k < c1; ++k)
                *(C + i * c2 + j) += *(A + i * c1 + k) * *(B + k * c2 + j);
        }
}

int main() {
    int f1 = 2, c1 = 3;
    int f2 = 3, c2 = 2;
    
    int A[f1 * c1] = {1, 2, 3, 4, 5, 6}; // 2x3 matriz
    int B[f2 * c2] = {7, 8, 9, 10, 11, 12}; // 3x2 matriz
    int* C = new int[f1 * c2];

    multiplicarMatrices(A, B, C, f1, c1, c2);

    cout << "Matriz resultado:\n";
    for (int i = 0; i < f1; ++i) {
        for (int j = 0; j < c2; ++j)
            cout << *(C + i * c2 + j) << " ";
        cout << endl;
    }
    delete[] C;
    return 0;
}

